# Copy to .env.local for local development
# Do NOT commit real secrets

# OpenAI (used by internal MCP route)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Supabase (frontend usage)
NEXT_PUBLIC_SUPABASE_URL=https://xxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=public-anon-key

# Optional external backend; leave empty to use internal Vercel route
# BACKEND_URL=https://your-backend.example.com

# Emotion/Personalization toggles (disable unless endpoints exist)
EMOTION_V2=false
CEL_V2_CAUSAL=false
EMOTION_PERSONALIZE=false
